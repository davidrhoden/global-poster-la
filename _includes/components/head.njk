<head>
  <meta charset="utf-8">
  <meta name="generator" content="{{ eleventy.generator }}">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Global Poster: {{ title or metadata.title }}</title>
  <meta name="description" content="{{ metaDescription or eleventyComputed.metaDescription or metadata.description }}">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Schibsted+Grotesk:ital,wght@0,400..900;1,400..900&display=swap" rel="stylesheet">
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {# Uncomment below if you want to include minified inline JavaScript #}
  {# {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script> #}
  <meta property="og:image" content="https://global-poster-la.com/static/img/global-poster-ten-commandments-1-2-merge.jpg">
  <meta property="og:image:alt" content="Government-mandated Ten Commandments posters now available from Global Poster.">
  <meta property="og:description" content="Government-approved Ten Commandments posters now available!">
  <meta property="og:title" content="Global Poster: {{ title or metadata.title }}">
  <meta name="twitter:title" content="Global Poster: {{ title or metadata.title }}">
  <meta name="twitter:description" content="Government-mandated Ten Commandments posters now available from Global Poster.">
  <meta name="twitter:image" content="https://global-poster-la.com/static/img/global-poster-ten-commandments-1-2-merge.jpg">
  <meta name="twitter:card" content="summary_large_image">
</head>
